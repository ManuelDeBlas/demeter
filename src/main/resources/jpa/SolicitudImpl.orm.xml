<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings
	xmlns="http://java.sun.com/xml/ns/persistence/orm"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm 
                                     http://java.sun.com/xml/ns/persistence/orm_1_0.xsd"
	version="1.0">

	<mapped-superclass
		class="es.mde.secres.SolicitudImpl" access="FIELD">
		<attributes>
			<basic name="nombreUco" />
			<basic name="ciu" />
			<basic name="estado" />
			<basic name="fechaInicio" />
			<basic name="fechaFin" />
			<basic name="coste" />
			<many-to-one name="reservista"
				target-entity="es.mde.entidades.ReservistaConId" />
			<many-to-one name="expediente"
				target-entity="es.mde.entidades.ExpedienteConId" />
		</attributes>
	</mapped-superclass>

</entity-mappings>
